    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "hono": "^4.6.1",
    "prisma": "^5.19.1",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  }
}
